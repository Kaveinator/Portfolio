{
  "Enabled": true,
  "Port": 80,
  "RequestTimeout": 10000,
  "ProductionDirectory": "Static",
  "DefaultDomain": "kavemans.dev",
  "ShowExceptionsOnErrorPages": true,
  "_UriFiller": "What files should it search for when autofilling URLs",
  "UriFillers": [
    "*.html",
    "*.htm",
    "*.txt",
    "index.html",
    "index.htm",
    "index.txt",
    "default.webp",
    "../default.webp",
    "default.png",
    "../default.png"
  ],
  "MaxCacheAge": 604800,
  "MaxConcurrentRequests": 1000
}